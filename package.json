{
  "name": "magi-app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest __tests__/services --coverage",
    "test:api": "jest __tests__/app/api --coverage",
    "test:integration": "jest __tests__/integration --coverage",
    "test:self-evolution": "jest __tests__/integration/self-evolution.test.ts --verbose",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:plan-gating": "jest __tests__/integration/plan-gating.test.ts --verbose",
    "test:usage": "jest __tests__/lib/usage --coverage",
    "test:middleware": "jest __tests__/lib/middleware --coverage",
    "test:admin": "jest __tests__/app/api/v1/admin --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "pnpm db:generate && pnpm db:push && pnpm db:seed",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "clean": "rm -rf .next",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "scan:semgrep": "semgrep --config .semgrep.yml",
    "scan:serena": "serena scan -c serena/serena.yml",
    "scan:all": "pnpm scan:semgrep && pnpm scan:serena",
    "security:check": "pnpm scan:all",
    "mcp:check": "node scripts/checkContext7.js",
    "mcp:health": "node scripts/checkContext7.js --health",
    "mcp:github": "npx -y @modelcontextprotocol/server-github",
    "mcp:neon": "npx -y @neondatabase/mcp-server-neon",
    "mcp:test": "node scripts/test-github-mcp.js",
    "mcp:test:neon": "node scripts/test-neon-mcp.js",
    "neon:status": "node scripts/neon-setup-status.js",
    "precommit": "pre-commit run --all-files",
    "security:install": "pip install semgrep && npm install -g @playwright/test"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.899.0",
    "@clerk/nextjs": "^6.32.2",
    "@google-cloud/bigquery": "^8.1.1",
    "@hookform/resolvers": "^5.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@neondatabase/mcp-server-neon": "^0.6.5",
    "@neondatabase/serverless": "^1.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-jaeger": "^2.1.0",
    "@opentelemetry/exporter-zipkin": "^2.1.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/glob": "^9.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.2",
    "glob": "^11.0.3",
    "lucide-react": "^0.544.0",
    "micro": "^10.0.1",
    "nanoid": "^5.1.6",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "snowflake-sdk": "^2.2.0",
    "sonner": "^2.0.7",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^30.1.3",
    "prisma": "^6.16.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
